{
  "WebSite": {
    "root": "",
    "host": "localhost:8000",
    "locdomain": "localhost"
  },
  "service": {
    "ip": "0.0.0.0",
    "api": "http",
    "port": 8000,
    "worker_processes": 5,
    "output_buffer_size": 32768
  },
  "logging": {
    "level": "error"
  },
  "http": {
    "script": "/mb.fcgi",
    "rewrite": [
      {
        "regex": "/static(/.+)",
        "pattern": "$1"
      },
      {
        "regex": ".*",
        "pattern": "/mb.fcgi$0"
      }
    ]
  },
  "file_server": {
    "enable": true,
    "listing": true,
    "document_root": "./static",
    "alias": [
      {
        "url": "/static",
        "path": "./static"
      }
    ]
  },
  "session": {
    "expire": "renew",
    "timeout": 604800,
    "location": "client",
    "client": {
      "hmac": "sha1",
      "hmac_key": "3891bbf7f845fd4277008a63d72640fc13bb9a31"
    }
  },
  "views": {
    "default_skin": "defskin",
    "paths": [
      "./build"
    ],
    "auto_reload": true
  }
}
